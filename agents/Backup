def _apply_tier_group_filter(self, rows: List[dict]) -> List[dict]:
    """
    Apply exclusive tier group filtering.
    
    Groups (exclusive - stops at first group with results):
        - Group A (Tier 1): Exact match only
        - Group B (Tier 2, 3): Prefix and contains phrase
        - Group C (Tier 4): All words with boundary
        - Group D (Tier 5): First N-1 words fallback
        - Group E (Tier 6): First 3 words fallback
    """
    
    # Single pass - group all rows by tier
    group_a_rows = []
    group_b_rows = []
    group_c_rows = []
    group_d_rows = []
    group_e_rows = []
    
    for r in rows:
        tier = r["tier"]
        if tier == 1:
            group_a_rows.append(r)
        elif tier in (2, 3):
            group_b_rows.append(r)
        elif tier == 4:
            group_c_rows.append(r)
        elif tier == 5:
            group_d_rows.append(r)
        elif tier == 6:
            group_e_rows.append(r)
    
    print(f"   ðŸ“Š Tier groups: A(1)={len(group_a_rows)}, B(2,3)={len(group_b_rows)}, C(4)={len(group_c_rows)}, D(5)={len(group_d_rows)}, E(6)={len(group_e_rows)}")
    
    # Return based on priority (exclusive)
    if group_a_rows:
        print(f"   âœ… Returning Group A (Tier 1 - Exact) results")
        return group_a_rows
    
    if group_b_rows:
        print(f"   âœ… Returning Group B (Tier 2,3 - Prefix/Contains phrase) results")
        return group_b_rows
    
    if group_c_rows:
        print(f"   âœ… Returning Group C (Tier 4 - All words boundary) results")
        return group_c_rows
    
    if group_d_rows:
        print(f"   âœ… Returning Group D (Tier 5 - First N-1 words) results")
        return group_d_rows
    
    if group_e_rows:
        print(f"   âœ… Returning Group E (Tier 6 - First 3 words) results")
        return group_e_rows
    
    return []
